<templateSet group="AndroidLog">
  <template name="mologi" value="android.util.Log.i(TAG, String.format(Locale.ENGLISH, &quot;$METHOD_NAME$()&quot;));" description="Log.i(TAG, String)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="svlogi" value="SVLog.i(TAG, String.format(Locale.ENGLISH, &quot;$METHOD_NAME$()&quot;));" description="SVLog.i(TAG, String)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="svlogd" value="SVLog.d(TAG, String.format(Locale.ENGLISH, &quot;$METHOD_NAME$()&quot;));" description="SVLog.d(TAG, String)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="svloge" value="SVLog.e(TAG, String.format(Locale.ENGLISH, &quot;$METHOD_NAME$()&quot;));" description="SVLog.e(TAG, String)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>